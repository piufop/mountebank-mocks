{
  "name": "mountebank-mocks",
  "version": "1.0.0",
  "description": "Mountebank mocks samples with NodeJS",
  "scripts": {
    "start": "nodemon --watch ./servers -e js,ejs,json --delay 1000ms --exec \"npm run start_mb\"",
    "start_mb": "mb --configfile servers/imposters.ejs --allowInjection --localOnly --nologfile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piufop/mountebank-mocks.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "mocks",
    "mock",
    "stub",
    "stubs",
    "mountebank"
  ],
  "author": "hugo.machado@outlook.com",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/piufop/mountebank-mocks/issues"
  },
  "homepage": "https://github.com/piufop/mountebank-mocks",
  "dependencies": {
    "mountebank": "^2.8.2",
    "nodemon": "^2.0.22"
  }
}
